msgstr "אישור"

#: public/js/drag_and_drop.js
msgid "Item Bank"
msgstr "בנק פריטים"

#: public/js/drag_and_drop.js
msgid "Placed in: {zone_title}"
msgstr "ממוקם ב: {zone_title}"

#: public/js/drag_and_drop.js
msgid "Correctly placed in: {zone_title}"
msgstr "ממוקם כהלכה ב: {zone_title}"

#: public/js/drag_and_drop.js
